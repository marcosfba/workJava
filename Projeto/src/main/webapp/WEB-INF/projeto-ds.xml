<?xml version="1.0" encoding="UTF-8"?>
<datasources xmlns="http://www.jboss.org/ironjacamar/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.jboss.org/ironjacamar/schema http://docs.jboss.org/ironjacamar/schema/datasources_1_0.xsd">
	<datasource jndi-name="java:jboss/datasources/ProjetoFinal"
		pool-name="ProjetoFinal">
		<connection-url>jdbc:mysql://localhost:3306/ProjetoFinal</connection-url>
		<driver>com.mysql</driver>
		<transaction-isolation>TRANSACTION_READ_COMMITTED</transaction-isolation>
		<pool>
			<min-pool-size>10</min-pool-size>
			<max-pool-size>100</max-pool-size>
			<prefill>true</prefill>
		</pool>
		<security>
			<user-name>root</user-name>
			<password></password>
		</security>
		<statement>
			<prepared-statement-cache-size>32</prepared-statement-cache-size>
			<share-prepared-statements />
		</statement>
	</datasource>
</datasources>
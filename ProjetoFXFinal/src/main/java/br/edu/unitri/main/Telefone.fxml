<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="br.edu.unitri.main.TelefoneFX">
	<top>
		<TabPane fx:id="tabTela" prefHeight="404.0" prefWidth="500.0"
			tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER"
			xmlns:fx="http://javafx.com/fxml">
			<tabs>
				<Tab fx:id="tabCadastro" text="Cadastro">
					<content>
						<Pane prefHeight="200.0" prefWidth="200.0">
							<children>
								<GridPane layoutX="93.0" layoutY="46.0" prefHeight="282.0"
									prefWidth="451.0" xmlns:fx="http://javafx.com/fxml">
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="185.0" minWidth="10.0" prefWidth="133.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											maxWidth="318.0" minWidth="10.0" prefWidth="318.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" xmlns="http://javafx.com/javafx/8"
											xmlns:fx="http://javafx.com/fxml/1" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" xmlns="http://javafx.com/javafx/8"
											xmlns:fx="http://javafx.com/fxml/1" />
										<RowConstraints minHeight="10.0" prefHeight="30.0"
											vgrow="SOMETIMES" xmlns="http://javafx.com/javafx/8"
											xmlns:fx="http://javafx.com/fxml/1" />
									</rowConstraints>
									<children>
										<Label text="Tipo de Telefone" />
										<Label text="Nº DDD:" GridPane.rowIndex="2" />
										<Label text="Nº Telefone:" GridPane.rowIndex="3" />
										<Label text="Pessoa" GridPane.rowIndex="1" />
										<Label text="Nº Ramal:" GridPane.rowIndex="4" />
										<Label text="Cód.Area:" GridPane.rowIndex="5" />
										<ComboBox fx:id="cbTipoTelefone" prefHeight="25.0"
											prefWidth="321.0" GridPane.columnIndex="1"
											xmlns:fx="http://javafx.com/fxml" />
										<ComboBox fx:id="cbPessoa" prefHeight="25.0"
											prefWidth="322.0" GridPane.columnIndex="1" GridPane.rowIndex="1"
											xmlns:fx="http://javafx.com/fxml" />
										<TextField fx:id="edNumDdd" GridPane.columnIndex="1"
											GridPane.rowIndex="2" />
										<TextField fx:id="edNumTelefone"
											GridPane.columnIndex="1" GridPane.rowIndex="3" />
										<TextField fx:id="edNumRamal" GridPane.columnIndex="1"
											GridPane.rowIndex="4" />
										<TextField fx:id="edCodArea" GridPane.columnIndex="1"
											GridPane.rowIndex="5" />
									</children>
								</GridPane>
								<Button fx:id="btnIncluir" layoutX="309.0" layoutY="336.0"
									mnemonicParsing="false" onAction="#btnIncluirClick" prefHeight="25.0"
									prefWidth="90.0" text="Salvar" />
								<Button fx:id="btnNovo" layoutX="202.0" layoutY="336.0"
									mnemonicParsing="false" onAction="#btnNovoClick" prefHeight="25.0"
									prefWidth="90.0" text="Novo" xmlns="http://javafx.com/javafx/8"
									xmlns:fx="http://javafx.com/fxml/1" />
							</children>
						</Pane>
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</top>
</BorderPane>

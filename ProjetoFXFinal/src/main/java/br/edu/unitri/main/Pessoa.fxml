<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.unitri.main.PessoaFX">
	<top>
		<TabPane fx:id="tabTela" prefHeight="404.0" prefWidth="500.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER" xmlns:fx="http://javafx.com/fxml">
			<tabs>
				<Tab fx:id="tabCadastro" text="Cadastro">
					<content>
						<Pane prefHeight="200.0" prefWidth="200.0">
							<children>
								<GridPane layoutX="93.0" layoutY="110.0" prefHeight="146.0" prefWidth="379.0" xmlns:fx="http://javafx.com/fxml">
									<columnConstraints>
										<ColumnConstraints hgrow="SOMETIMES" maxWidth="185.0" minWidth="10.0" prefWidth="98.0" />
										<ColumnConstraints hgrow="SOMETIMES" maxWidth="293.0" minWidth="10.0" prefWidth="281.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
										<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
									</rowConstraints>
									<children>
										<Label text="Nome">
										</Label>
										<Label text="Tipo de Pessoa" GridPane.rowIndex="1">
										</Label>
										<Label text="Nascimento" GridPane.rowIndex="2">
										</Label>
										<TextField fx:id="txtNome" GridPane.columnIndex="1" />
										<ComboBox fx:id="cbTipoPessoa" prefHeight="25.0" prefWidth="283.0" GridPane.columnIndex="1" GridPane.rowIndex="1" xmlns:fx="http://javafx.com/fxml" />
										<DatePicker fx:id="dtNascimento" prefHeight="25.0" prefWidth="281.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
									</children>
								</GridPane>
								<Button fx:id="btnIncluir" layoutX="328.0" layoutY="284.0" mnemonicParsing="false" onAction="#btnIncluirClick" prefHeight="25.0" prefWidth="90.0" text="Salvar" />
								<Button fx:id="btnNovo" layoutX="221.0" layoutY="284.0" mnemonicParsing="false" onAction="#btnNovoClick" prefHeight="25.0" prefWidth="90.0" text="Novo" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" />
							</children>
						</Pane>
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</top>
</BorderPane>
